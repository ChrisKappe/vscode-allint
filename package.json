{
    "name": "nav-skills-al-clean-code",
    "displayName": "NAV Skills AL Clean Code",
    "description": "Check your code for maintainability and upgradability",
    "version": "0.1.0",
    "publisher": "MarkBrummel",
    "icon": "ALLint_icon.png",
    "engines": {
        "vscode": "^1.14.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/markbrummel/vscode-allint.git"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:Refactor",
        "onCommand:CleanCode",
        "onLanguage:al"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "Refactor",
                "title": "AL: Refactor (NAV-Skills)"
            },
            {
                "command": "CleanCode",
                "title": "AL: Clean Code (NAV-Skills)"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}